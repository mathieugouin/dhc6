<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <name>Starting the engines</name>

    <description>We have finished the Preflight inspection and the pre-startup procedure right now. At this point it's time for you to learn how to start the engines.</description>

    <init>
        <set>
            <property>/sim/current-view/view-number</property>
            <value>0</value>
        </set>
        <set>
            <property>/controls/gear/parkingbrake-lever</property>
            <value>1</value>
        </set>
        <set>
            <property>/controls/engines/engine/cutoff</property>
            <value>false</value>
        </set>
        <set>
            <property>/controls/engines/engine[1]/cutoff</property>
            <value>false</value>
        </set>
        <set>
            <property>/controls/fuel/tank-selector</property>
            <value>0</value>
        </set>
        <set>
            <property>/controls/electric/engine/generator</property>
            <value>false</value>
        </set>
        <set>
            <property>/controls/electric/engine[1]/generator</property>
            <value>false</value>
        </set>
        <set>
            <property>/controls/flight/flaps</property>
            <value>0</value>
        </set>
        <set>
            <property>/controls/engines/engine/condition</property>
            <value>0</value>
        </set>
        <set>
            <property>/controls/engines/engine[1]/condition</property>
            <value>0</value>
        </set>
        <set>
            <property>/controls/engines/engine/propeller-pitch</property>
            <value>0</value>
        </set>
        <set>
            <property>/controls/engines/engine[1]/propeller-pitch</property>
            <value>0</value>
        </set>
        <set>
            <property>/controls/engines/engine/throttle</property>
            <value>0</value>
        </set>
        <set>
            <property>/controls/engines/engine[1]/throttle</property>
            <value>0</value>
        </set>
        <set>
            <property>/controls/lighting/beacon</property>
            <value>false</value>
        </set>
        <set>
            <property>/controls/lighting/landing-light</property>
            <value>false</value>
        </set>
        <set>
            <property>/controls/lighting/landing-light[1]</property>
            <value>false</value>
        </set>
        <set>
            <property>/controls/lighting/nav-lights</property>
            <value>false</value>
        </set>
        <set>
            <property>/controls/lighting/strobe</property>
            <value>false</value>
        </set>
        <set>
            <property>/controls/lighting/taxi-lights</property>
            <value>false</value>
        </set>
        <set>
            <property>/controls/electric/caution-test</property>
            <value>false</value>
        </set>
        <set>
            <property>/controls/electric/power-source</property>
            <value>-1</value>
        </set>
        <set>
            <property>/sim/model/equipment/ground-services/external-power/enable</property>
            <value>true</value>
        </set>
        <set>
            <property>/controls/electric/battery-switch</property>
            <value>true</value>
        </set>
        <set>
            <property>/controls/lighting/cabin-lights</property>
            <value>true</value>
        </set>
        <set>
            <property>/controls/lighting/instrument-lights</property>
            <value>1</value>
        </set>
        <set>
            <property>/controls/electric/ammeter-switch</property>
            <value>false</value>
        </set>
    </init>

    <step>
        <message>Here I have the engine start checklist, so let's start the engines :)</message>
        <wait>5</wait>
    </step>

    <step>
        <message>Turn on the beacon</message>
        <view>
            <heading-offset-deg>246.89</heading-offset-deg>
            <pitch-offset-deg>27.46</pitch-offset-deg>
            <roll-offset-deg>0</roll-offset-deg>
            <x-offset-m>-0.36</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>25</field-of-view>
        </view>
        <marker>
            <x-m>-2.9884</x-m>
            <y-m>0.0174</y-m>
            <z-m>0.9397</z-m>
            <scale>1.2810</scale>
        </marker>
        <error>
            <message>Before you can start the engines you must turn on the beacon to warn everyone near that soon the engines will start</message>
            <condition>
                <not>
                    <property>/controls/lighting/beacon</property>
                </not>
            </condition>
        </error>
        <exit>
            <condition>
                <property>/controls/lighting/beacon</property>
            </condition>
        </exit>
    </step>

    <step>
        <message>Verify that the throttles are in Idle position</message>
        <view>
            <heading-offset-deg>303.6</heading-offset-deg>
            <pitch-offset-deg>16.6</pitch-offset-deg>
            <roll-offset-deg>0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>45</field-of-view>
        </view>
        <marker>
               <x-m>-3.4157</x-m>
            <y-m>-0.1165</y-m>
               <z-m>0.8488</z-m>
               <scale>3.3471</scale>
        </marker>
        <error>
            <message>Pull the left throttle to idle!</message>
            <condition>
                <not-equals>
                    <property>/controls/engines/engine/throttle</property>
                    <value>0</value>
                </not-equals>
            </condition>
        </error>
        <error>
            <message>Pull the right throttle to idle!</message>
            <condition>
                <not-equals>
                    <property>/controls/engines/engine[1]/throttle</property>
                    <value>0</value>
                </not-equals>
            </condition>
        </error>
        <exit>
            <condition>
                <and>
                    <equals>
                        <property>/controls/engines/engine/throttle</property>
                        <value>0</value>
                    </equals>
                    <equals>
                        <property>/controls/engines/engine[1]/throttle</property>
                        <value>0</value>
                    </equals>
                </and>
            </condition>
        </exit>
    </step>

    <step>
        <message>If you do NOT start up on asphalt, you have to switch on the intake deflectors. Otherwise the engines will fail!</message>
        <view>
            <heading-offset-deg>229</heading-offset-deg>
            <pitch-offset-deg>30</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.32</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>40</field-of-view>
        </view>
        <marker>
            <x-m>-2.9207</x-m>
            <y-m>-0.0636</y-m>
            <z-m>0.9306</z-m>
               <scale>2.0248</scale>
        </marker>
        <wait>10</wait>
    </step>

    <step>
        <message>Turn on both boost pumps</message>
        <view>
            <heading-offset-deg>335</heading-offset-deg>
            <pitch-offset-deg>-33</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>25</field-of-view>
        </view>
        <error>
            <message>Turn on the AFT boost pump</message>
            <condition>
                <not-equals>
                    <property>/controls/electric/aft-boost-pump</property>
                    <value>1</value>
                </not-equals>
            </condition>
        </error>
        <error>
            <message>Turn on the FWD boost pump</message>
            <condition>
                <not-equals>
                    <property>/controls/electric/fwd-boost-pump</property>
                    <value>1</value>
                </not-equals>
            </condition>
        </error>
        <exit>
            <condition>
                <and>
                    <equals>
                        <property>/controls/electric/aft-boost-pump</property>
                        <value>1</value>
                    </equals>
                    <equals>
                        <property>/controls/electric/fwd-boost-pump</property>
                        <value>1</value>
                    </equals>
                </and>
            </condition>
        </exit>
    </step>

    <step>
        <message>If the outside temperature is less than 0 deg Celsius</message>
        <view>
            <heading-offset-deg>345</heading-offset-deg>
            <pitch-offset-deg>57</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>75</field-of-view>
        </view>
        <marker>
            <x-m>-3.3430</x-m>
            <y-m>-0.4215</y-m>
            <z-m>0.9926</z-m>
            <scale>3.6281</scale>
        </marker>
        <wait>4</wait>
    </step>

    <step>
        <message>Switch on the Pitot Heat...</message>
        <view>
            <heading-offset-deg>253</heading-offset-deg>
            <pitch-offset-deg>28.5</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.38</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>40</field-of-view>
        </view>
        <marker>
            <x-m>-3.0347</x-m>
            <y-m>0.0149</y-m>
            <z-m>0.9355</z-m>
            <scale>1.6281</scale>
        </marker>
        <error>
            <message>Because it's less than 0 deg Celsius outside, you have to switch on the pitot heat</message>
            <condition>
                <and>
                    <less-than-equals>
                        <property>/environment/temperature-degc</property>
                        <value>0</value>
                    </less-than-equals>
                    <not>
                        <property>/controls/anti-ice/pitot-heat</property>
                    </not>
                </and>
            </condition>
        </error>
        <error>
            <message>We don't need the pitot heat, because it's warm enough outside, so switch it off</message>
            <condition>
                <and>
                    <greater-than>
                        <property>/environment/temperature-degc</property>
                        <value>0</value>
                    </greater-than>
                    <property>/controls/anti-ice/pitot-heat</property>
                </and>
            </condition>
        </error>
        <exit>
            <condition>
                <or>
                    <and>
                        <less-than-equals>
                            <property>/environment/temperature-degc</property>
                            <value>0</value>
                        </less-than-equals>
                        <property>/controls/anti-ice/pitot-heat</property>
                    </and>
                    <and>
                        <greater-than>
                            <property>/environment/temperature-degc</property>
                            <value>0</value>
                        </greater-than>
                        <not>
                            <property>/controls/anti-ice/pitot-heat</property>
                        </not>
                    </and>
                </or>
            </condition>
        </exit>
    </step>

    <step>
        <message>...and the prop-deice</message>
        <view>
            <heading-offset-deg>220</heading-offset-deg>
            <pitch-offset-deg>35</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.3</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>56</field-of-view>
        </view>
        <marker>
            <x-m>-2.9041</x-m>
            <y-m>-0.1132</y-m>
            <z-m>0.9446</z-m>
            <scale>1.7273</scale>
        </marker>
        <error>
            <message>Because it's less than 0 deg Celsius outside, you have to switch on the prop-deice</message>
            <condition>
                <and>
                    <less-than-equals>
                        <property>/environment/temperature-degc</property>
                        <value>0</value>
                    </less-than-equals>
                    <not>
                        <property>/controls/anti-ice/prop-heat</property>
                    </not>
                </and>
            </condition>
        </error>
        <error>
            <message>We don't need the prop-deice, because it's warm enough outside, so switch it off</message>
            <condition>
                <and>
                    <greater-than>
                        <property>/environment/temperature-degc</property>
                        <value>0</value>
                    </greater-than>
                    <property>/controls/anti-ice/prop-heat</property>
                </and>
            </condition>
        </error>
        <exit>
            <condition>
                <or>
                    <and>
                        <less-than-equals>
                            <property>/environment/temperature-degc</property>
                            <value>0</value>
                        </less-than-equals>
                        <property>/controls/anti-ice/prop-heat</property>
                    </and>
                    <and>
                        <greater-than>
                            <property>/environment/temperature-degc</property>
                            <value>0</value>
                        </greater-than>
                        <not>
                            <property>/controls/anti-ice/prop-heat</property>
                        </not>
                    </and>
                </or>
            </condition>
        </exit>
    </step>

    <step>
        <message>Make sure that the left propeller area is clear, so that no one's going to walk into the rotating propeller</message>
        <view>
            <heading-offset-deg>100</heading-offset-deg>
            <pitch-offset-deg>2</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.57</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.38</z-offset-m>
            <field-of-view>100</field-of-view>
        </view>
        <wait>5</wait>
    </step>

    <step>
        <message>Propeller clear? Good, then pick starter switch left (LMB)</message>
        <view>
            <heading-offset-deg>341.8</heading-offset-deg>
            <pitch-offset-deg>22.2</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>35</field-of-view>
        </view>
        <marker>
            <x-m>-3.6124</x-m>
            <y-m>-0.2686</y-m>
            <z-m>0.8992</z-m>
            <scale>1.4793</scale>
        </marker>
        <error>
            <message>To start the engine, you have to set the starter switch to the left by pressing the LMB</message>
            <condition>
                <not-equals>
                    <property>/controls/engines/internal-engine-starter</property>
                    <value>1</value>
                </not-equals>
            </condition>
        </error>
        <exit>
            <condition>
                <equals>
                    <property>/controls/engines/internal-engine-starter</property>
                    <value>1</value>
                </equals>
            </condition>
        </exit>
    </step>

    <step>
        <message>Let the left engine spool up to 12 percent GG RPM</message>
        <view>
            <heading-offset-deg>339.33</heading-offset-deg>
            <pitch-offset-deg>-29.21</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>35</field-of-view>
        </view>
        <marker>
            <x-m>-3.9339</x-m>
            <y-m>-0.1537</y-m>
            <z-m>0.2314</z-m>
            <scale>2.8347</scale>
        </marker>
        <exit>
            <condition>
                <greater-than>
                    <property>/engines/engine/n2</property>
                    <value>12</value>
                </greater-than>
            </condition>
        </exit>
    </step>

    <step>
        <message>Push the left condition full forward ("j"-key)</message>
        <view>
            <heading-offset-deg>301.5</heading-offset-deg>
            <pitch-offset-deg>15.18</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>55</field-of-view>
        </view>
        <marker>
              <x-m>-3.3149</x-m>
               <y-m>0.0826</y-m>
            <z-m>0.8694</z-m>
               <scale>2.1074</scale>
        </marker>
        <error>
            <message>To supply the engine with fuel, you have to push the left condition full forward (press "j" several times)</message>
            <condition>
                <less-than>
                    <property>/controls/engines/engine/condition</property>
                    <value>1</value>
                </less-than>
            </condition>
        </error>
        <exit>
            <condition>
                <equals>
                    <property>/controls/engines/engine/condition</property>
                    <value>1</value>
                </equals>
            </condition>
        </exit>
    </step>

    <step>
        <message>Give the left engine 5 secs to spool up fully</message>
        <view>
            <heading-offset-deg>339.33</heading-offset-deg>
            <pitch-offset-deg>-29.21</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>50</field-of-view>
        </view>
        <wait>5</wait>
    </step>

    <step>
        <message>Now I verify the right prop is clear...</message>
        <view>
            <heading-offset-deg>333.5</heading-offset-deg>
            <pitch-offset-deg>-12.4</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>90</field-of-view>
        </view>
        <wait>5</wait>
    </step>

    <step>
        <message>Looks good at my site, too, so you can pick the starter switch right</message>
        <view>
            <heading-offset-deg>341.8</heading-offset-deg>
            <pitch-offset-deg>22.2</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>35</field-of-view>
        </view>
        <marker>
            <x-m>-3.6124</x-m>
            <y-m>-0.2686</y-m>
            <z-m>0.8992</z-m>
            <scale>1.4793</scale>
        </marker>
        <error>
            <message>To select the right starter, click the starter switch twice with MMB</message>
            <condition>
                <not-equals>
                    <property>/controls/engines/internal-engine-starter</property>
                    <value>-1</value>
                </not-equals>
            </condition>
        </error>
        <exit>
            <condition>
                <equals>
                    <property>/controls/engines/internal-engine-starter</property>
                    <value>-1</value>
                </equals>
            </condition>
        </exit>
    </step>

    <step>
        <message>Now let the right engine spool up to 12 percent GG RPM</message>
        <view>
            <heading-offset-deg>339.33</heading-offset-deg>
            <pitch-offset-deg>-29.21</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>35</field-of-view>
        </view>
        <marker>
            <x-m>-3.9264</x-m>
            <y-m>-0.0793</y-m>
            <z-m>0.2347</z-m>
            <scale>2.7851</scale>
        </marker>
        <exit>
            <condition>
                <greater-than>
                    <property>/engines/engine[1]/n2</property>
                    <value>12</value>
                </greater-than>
            </condition>
        </exit>
    </step>

    <step>
        <message>Push the right condition full forward ("k"-key)</message>
        <view>
            <heading-offset-deg>301.5</heading-offset-deg>
            <pitch-offset-deg>15.18</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>55</field-of-view>
        </view>
        <marker>
              <x-m>-3.3149</x-m>
               <y-m>0.0826</y-m>
            <z-m>0.8694</z-m>
               <scale>2.1074</scale>
        </marker>
        <error>
            <message>To supply the engine with fuel, you have to push the right condition full forward (press "k" several times)</message>
            <condition>
                <less-than>
                    <property>/controls/engines/engine[1]/condition</property>
                    <value>1</value>
                </less-than>
            </condition>
        </error>
        <exit>
            <condition>
                <equals>
                    <property>/controls/engines/engine[1]/condition</property>
                    <value>1</value>
                </equals>
            </condition>
        </exit>
    </step>

    <step>
        <message>Give also the right engine 5 secs to spool up fully</message>
        <view>
            <heading-offset-deg>339.33</heading-offset-deg>
            <pitch-offset-deg>-29.21</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>50</field-of-view>
        </view>
        <wait>5</wait>
    </step>

    <step>
        <message>Set the starter back to off</message>
        <view>
            <heading-offset-deg>341.8</heading-offset-deg>
            <pitch-offset-deg>22.2</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>35</field-of-view>
        </view>
        <marker>
            <x-m>-3.6124</x-m>
            <y-m>-0.2686</y-m>
            <z-m>0.8992</z-m>
            <scale>1.4793</scale>
        </marker>
        <error>
            <message>Now, as both engines are running, we don't need the starter anymore, so turn it off</message>
            <condition>
                <not-equals>
                    <property>/controls/engines/internal-engine-starter</property>
                    <value>0</value>
                </not-equals>
            </condition>
        </error>
        <exit>
            <condition>
                <equals>
                    <property>/controls/engines/internal-engine-starter</property>
                    <value>0</value>
                </equals>
            </condition>
        </exit>
    </step>

    <step>
        <message>You can now push the propeller levers full forward.</message>
        <view>
            <heading-offset-deg>301.5</heading-offset-deg>
            <pitch-offset-deg>15.18</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>55</field-of-view>
        </view>
        <marker>
            <x-m>-3.3174</x-m>
               <y-m>-0.0132</y-m>
               <z-m>0.8612</z-m>
               <scale>2.5207</scale>
        </marker>
        <error>
            <message>For maximum thrust at take off, the prop levers must be full forward (hold "n")</message>
            <condition>
                <and>
                    <less-than>
                        <property>/controls/engines/engine/propeller-pitch</property>
                        <value>1</value>
                    </less-than>
                    <less-than>
                        <property>/controls/engines/engine[1]/propeller-pitch</property>
                        <value>1</value>
                    </less-than>
                </and>
            </condition>
        </error>
        <exit>
            <condition>
                <and>
                    <equals>
                        <property>/controls/engines/engine/propeller-pitch</property>
                        <value>1</value>
                    </equals>
                    <equals>
                        <property>/controls/engines/engine[1]/propeller-pitch</property>
                        <value>1</value>
                    </equals>
                </and>
            </condition>
        </exit>
    </step>

    <step>
        <message>The engines are running now, but still you can't take off, so continue with the after engine start checklist</message>
        <view>
            <heading-offset-deg>0</heading-offset-deg>
            <pitch-offset-deg>-15</pitch-offset-deg>
            <roll-offset-deg>0.0</roll-offset-deg>
            <x-offset-m>-0.48</x-offset-m>
            <y-offset-m>0.72</y-offset-m>
            <z-offset-m>-3.15</z-offset-m>
            <field-of-view>85</field-of-view>
        </view>
        <wait>5</wait>
    </step>

    <end>
    </end>

</PropertyList>
