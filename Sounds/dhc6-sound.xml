<PropertyList>

    <fx>
        <fire>
            <name>fire-test</name>
            <path>Aircraft/dhc6/Sounds/dhc6-alarm.wav</path>
            <mode>looped</mode>
            <property>controls/engines/fire-test</property>
            <condition>
                <and>
                    <equals>
                        <property>controls/engines/fire-test</property>
                        <value>1</value>
                    </equals>
                    <greater-than>
                        <property>systems/electrical/outputs/fire-bell</property>
                        <value>0</value>
                    </greater-than>
                    <not>
                        <property>controls/switches/firebell-switch</property>
                    </not>
                </and>
            </condition>
            <volume>
               <factor>3</factor>
            </volume>
            <position>
                <x>-4</x>
                <y>0</y>
                <z>-0.6</z>
            </position>
            <reference-dist>0.2</reference-dist>
            <max-dist>10</max-dist>
        </fire>

        <!-- Cabin Alarms -->
        <cabin>
            <name>seat-belt</name>
            <path>Aircraft/dhc6/Sounds/dhc6-cabinalert.wav</path>
            <mode>once</mode>
            <property>controls/lighting/seat-belt</property>
            <condition>
                <and>
                    <equals>
                        <property>controls/lighting/seat-belt</property>
                        <value>1</value>
                    </equals>
                    <greater-than>
                        <property>systems/electrical/outputs/seatbelt</property>
                        <value>0</value>
                    </greater-than>
                </and>
            </condition>
            <volume>
                <factor>0.3</factor>
            </volume>
            <position>
                <x>-2.8</x>
                <y>-0.5</y>
                <z>0.6</z>
            </position>
            <orientation>
                <x>1</x>
                <y>0</y>
                <z>0</z>
            </orientation>
        </cabin>

        <cabin>
            <name>seat-belt-off</name>
            <path>Aircraft/dhc6/Sounds/dhc6-cabinalert.wav</path>
            <mode>once</mode>
            <property>controls/lighting/seat-belt</property>
            <condition>
                <and>
                    <equals>
                        <property>controls/lighting/seat-belt</property>
                        <value>0</value>
                    </equals>
                    <greater-than>
                        <property>systems/electrical/buses/R-28V-DC-volts</property>
                        <value>0</value>
                    </greater-than>
                </and>
            </condition>
            <volume>
                <factor>0.3</factor>
            </volume>
            <position>
                <x>-2.8</x>
                <y>-0.5</y>
                <z>0.6</z>
            </position>
            <orientation>
                <x>1</x>
                <y>0</y>
                <z>0</z>
            </orientation>
        </cabin>

        <cabin>
            <name>no-smoking</name>
            <path>Aircraft/dhc6/Sounds/dhc6-cabinalert.wav</path>
            <mode>once</mode>
            <property>controls/lighting/no-smoking</property>
            <condition>
                <and>
                    <equals>
                        <property>controls/lighting/no-smoking</property>
                        <value>1</value>
                    </equals>
                    <greater-than>
                        <property>systems/electrical/outputs/no-smoking</property>
                        <value>0</value>
                    </greater-than>
                </and>
            </condition>
            <volume>
                <factor>0.3</factor>
            </volume>
            <position>
                <x>-2.8</x>
                <y>-0.5</y>
                <z>0.6</z>
            </position>
            <orientation>
                <x>1</x>
                <y>0</y>
                <z>0</z>
            </orientation>
        </cabin>

        <cabin>
            <name>no-smoking-off</name>
            <path>Aircraft/dhc6/Sounds/dhc6-cabinalert.wav</path>
            <mode>once</mode>
            <property>controls/lighting/no-smoking</property>
            <condition>
                <and>
                    <equals>
                        <property>controls/lighting/no-smoking</property>
                        <value>0</value>
                    </equals>
                    <greater-than>
                        <property>systems/electrical/buses/R-28V-DC-volts</property>
                        <value>0</value>
                    </greater-than>
                </and>
            </condition>
            <volume>
                <factor>0.3</factor>
            </volume>
            <position>
                <x>-2.8</x>
                <y>-0.5</y>
                <z>0.6</z>
            </position>
            <orientation>
                <x>1</x>
                <y>0</y>
                <z>0</z>
            </orientation>
        </cabin>

        <stall>
            <name>stall-warning</name>
            <mode>looped</mode>
            <path>Aircraft/dhc6/Sounds/stall.wav</path>
            <property>sim/sound/stall-warning</property>
            <condition>
                <or>
                    <and>
                        <greater-than>
                            <property>systems/electrical/outputs/stall-warn</property>
                            <value>0</value>
                        </greater-than>
                        <greater-than>
                            <property>sim/sound/stall-warning</property>
                            <value>4</value>
                        </greater-than>
                        <less-than>
                            <property>sim/sound/stall-warning</property>
                            <value>20</value>
                        </less-than>
                        <greater-than>
                            <property>sim/time/elapsed-sec</property>
                            <value>15</value>
                        </greater-than>
                        <greater-than>
                            <property>velocities/airspeed-kt</property>
                            <value>30</value>
                        </greater-than>
                        <property>sim/current-view/internal</property>
                    </and>
                    <!-- caution test activates stall horn if circuit breaker is in -->
                    <and>
                        <greater-than>
                            <property>controls/electric/caution-test</property>
                            <value>0</value>
                        </greater-than>
                        <greater-than>
                            <property>systems/electrical/outputs/stall-warn</property>
                            <value>0</value>
                        </greater-than>
                    </and>
                </or>
            </condition>
            <volume>
                <property>sim/sound/stall-warning</property>
                <factor>0.05</factor>
            </volume>
            <pitch>
                <property>sim/sound/stall-warning</property>
                <factor>0.05</factor>
            </pitch>
            <reference-dist>3.0</reference-dist>
            <max-dist>10.0</max-dist>
            <position>
                <x>-4</x>
                <y>0</y>
                <z>-0.6</z>
            </position>
        </stall>

        <!-- WIPERS -->
        <wipers>
            <name>wiper</name>
            <path>Aircraft/dhc6/Sounds/dhc6-wipers.wav</path>
            <mode>looped</mode>
            <property>controls/electric/wipers/switch</property>
            <condition>
                <and>
                    <equals>
                        <property>controls/electric/wipers/switch</property>
                        <value>1</value>
                    </equals>
                    <equals>
                        <property>sim/current-view/internal</property>
                        <value>1</value>
                    </equals>
                    <greater-than>
                        <property>systems/electrical/volts</property>
                        <value>0</value>
                    </greater-than>
                </and>
            </condition>
            <volume>
                <factor>0.5</factor>
            </volume>
        </wipers>

        <prop>
            <name>aft-boost-pump</name>
            <path>Aircraft/dhc6/Sounds/dhc6-boost-pump.wav</path>
            <mode>looped</mode>
            <property>sim/sound/aft-boost-pump</property>
            <condition>
                <and>
                    <equals>
                        <property>controls/electric/aft-boost-pump</property>
                        <value>1</value>
                    </equals>
                    <greater-than>
                        <property>systems/electrical/volts</property>
                        <value>0</value>
                    </greater-than>
                    <property>sim/current-view/internal</property>
                </and>
            </condition>
            <volume>
                <factor>0.16</factor>
            </volume>
        </prop>

        <prop>
            <name>fwd-boost-pump</name>
            <path>Aircraft/dhc6/Sounds/dhc6-boost-pump.wav</path>
            <mode>looped</mode>
            <property>sim/sound/fwd-boost-pump</property>
            <condition>
                <and>
                    <equals>
                        <property>controls/electric/fwd-boost-pump</property>
                        <value>1</value>
                    </equals>
                    <greater-than>
                        <property>systems/electrical/volts</property>
                        <value>0</value>
                    </greater-than>
                    <property>sim/current-view/internal</property>
                </and>
            </condition>
            <volume>
                <factor>0.16</factor>
            </volume>
        </prop>
    
        <prop>
            <name>starter</name>
            <path>Aircraft/dhc6/Sounds/dhc6-engine-starter.wav</path>
            <mode>once</mode>
            <property>sim/sound/engine-starter</property>
            <condition>
                <and>
                    <not>
                        <equals>
                            <property>controls/engines/internal-engine-starter</property>
                            <value>0</value>
                        </equals>
                    </not>
                    <not>
                        <equals>
                            <property>controls/electric/power-source</property>
                            <value>0</value>
                        </equals>
                    </not>
                    <or>
                        <greater-than>
                            <property>systems/electrical/outputs/starter-l</property>
                            <value>0</value>
                        </greater-than>
                        <greater-than>
                            <property>systems/electrical/outputs/starter-r</property>
                            <value>0</value>
                        </greater-than>
                    </or>
                </and>
            </condition>
            <volume>
                <factor>0.3</factor>
            </volume>
        </prop>

        <!-- Engine 1 exterior-->
        <prop>
            <name>propeller1</name>
            <mode>looped</mode>
            <path>Aircraft/dhc6/Sounds/fullpower.wav</path>
            <property>sim/sound/engine[0]</property>
            <condition>
                <and>
                    <greater-than>
                        <property>controls/engines/engine[0]/condition</property>
                        <value>0.001</value>
                    </greater-than>
                    <not>
                        <property>sim/current-view/internal</property>
                    </not>
                </and>
            </condition>
            <volume>
                <property>sim/sound/engine[0]</property>
                <factor>5</factor>
                <offset>0</offset>
            </volume>
            <pitch>
                <property>sim/sound/engine[0]</property>
                <factor>1.1</factor>
                <offset>0</offset>
            </pitch>
            <position>
                <x>-1.5</x>
                <y>2.6</y>
                <z>-0.33</z>
            </position>
            <reference-dist>20.0</reference-dist>
            <max-dist>5000.0</max-dist>
        </prop>

        <prop>
            <name>propeller1-int</name>
            <reference-dist>2.0</reference-dist>
            <max-dist>10.0</max-dist>
            <mode>looped</mode>
            <path>Aircraft/dhc6/Sounds/fullpower-int.wav</path>
            <property>sim/sound/engine[0]</property>
            <condition>
                <or>
                    <and>
                        <equals>
                            <property>sim/flight-model</property>
                            <value>jsb</value>
                        </equals>
                        <greater-than>
                            <property>controls/engines/engine[0]/condition</property>
                            <value>0.001</value>
                        </greater-than>
                        <not>
                            <property>controls/engines/engine[0]/cutoff</property>
                        </not>
                        <property>sim/current-view/internal</property>
                    </and>
                    <and>
                        <equals>
                            <property>sim/flight-model</property>
                            <value>yasim</value>
                        </equals>
                        <greater-than>
                            <property>controls/engines/engine[0]/condition</property>
                            <value>0.001</value>
                        </greater-than>
                        <greater-than>
                            <property>engines/engine[0]/n2</property>
                            <value>10</value>
                        </greater-than>
                        <not>
                            <property>controls/engines/engine[0]/cutoff</property>
                        </not>
                        <property>sim/current-view/internal</property>
                    </and>
                </or>
            </condition>
            <volume>
                <property>sim/sound/engine[0]</property>
                <factor>5</factor>
                <offset>0</offset>
            </volume>
            <pitch>
                <property>sim/sound/engine[0]</property>
                <factor>1.1</factor>
                <offset>0</offset>
            </pitch>
            <position>
                <x>-1.5</x>
                <y>2.6</y>
                <z>-0.33</z>
            </position>
        </prop>

        <!-- Engine 2 -->
        <prop>
            <name>propeller2</name>
            <reference-dist>20.0</reference-dist>
            <max-dist>5000.0</max-dist>
            <mode>looped</mode>
            <path>Aircraft/dhc6/Sounds/fullpower.wav</path>
            <property>sim/sound/engine[1]</property>
            <condition>
                <and>
                    <greater-than>
                        <property>controls/engines/engine[1]/condition</property>
                        <value>0.001</value>
                    </greater-than>
                    <not>
                        <property>sim/current-view/internal</property>
                    </not>
                </and>
            </condition>
            <volume>
                <property>sim/sound/engine[1]</property>
                <factor>5</factor>
                <offset>0</offset>
            </volume>
            <pitch>
                <property>sim/sound/engine[1]</property>
                <factor>1.1</factor>
                <offset>0</offset>
            </pitch>
            <position>
                <x>-1.5</x>
                <y>-2.6</y>
                <z>-0.33</z>
            </position>
        </prop>

        <prop>
            <name>propeller2-int</name>
            <reference-dist>2.0</reference-dist>
            <max-dist>10.0</max-dist>
            <mode>looped</mode>
            <path>Aircraft/dhc6/Sounds/fullpower-int.wav</path>
            <property>sim/sound/engine[1]</property>
            <condition>
                <or>
                    <and>
                        <equals>
                            <property>sim/flight-model</property>
                            <value>jsb</value>
                        </equals>
                        <greater-than>
                            <property>controls/engines/engine[1]/condition</property>
                            <value>0.001</value>
                        </greater-than>
                        <not>
                            <property>controls/engines/engine[1]/cutoff</property>
                        </not>
                        <property>sim/current-view/internal</property>
                    </and>
                    <and>
                        <equals>
                            <property>sim/flight-model</property>
                            <value>yasim</value>
                        </equals>
                        <greater-than>
                            <property>controls/engines/engine[1]/condition</property>
                            <value>0.001</value>
                        </greater-than>
                        <greater-than>
                            <property>engines/engine[1]/n2</property>
                            <value>10</value>
                        </greater-than>
                        <not>
                            <property>controls/engines/engine[1]/cutoff</property>
                        </not>
                        <property>sim/current-view/internal</property>
                    </and>
                </or>
            </condition>
            <volume>
                <property>sim/sound/engine[1]</property>
                <factor>5</factor>
                <offset>0</offset>
            </volume>
            <pitch>
                <property>sim/sound/engine[1]</property>
                <factor>1.1</factor>
                <offset>0</offset>
            </pitch>
            <position>
                <x>-1.5</x>
                <y>2.6</y>
                <z>-0.33</z>
            </position>
        </prop>

        <rumble>
            <name>rumble</name>
            <reference-dist>5.0</reference-dist>
            <max-dist>20.0</max-dist>
            <mode>looped</mode>
            <path>Aircraft/dhc6/Sounds/rumble.wav</path>
            <condition>
                <or>
                    <property>gear/gear[0]/wow</property>
                    <property>gear/gear[1]/wow</property>
                    <property>gear/gear[2]/wow</property>
                    <property>gear/gear[3]/wow</property>
                </or>
            </condition>
            <volume>
                <property>velocities/groundspeed-kt</property>
                <factor>0.05</factor>
                <max>1</max>
            </volume>
            <position>
                <x>-3</x>
                <y>0</y>
                <z>-1</z>
            </position>
        </rumble>

        <gear>
            <name>gear</name>
            <mode>in-transit</mode>
            <path>Sounds/gear.wav</path>
            <property>/gear/gear[5]/position-norm</property>
            <volume>
                <property>sim/sound/cabin</property>
            </volume>
            <pitch>
                <internal>dt_play</internal>
                <offset>1.0</offset>
                <factor>-0.001</factor>
            </pitch>
            <position>
                <x>-3</x>
                <y>0</y>
                <z>0.5</z>
            </position>
            <reference-dist>1.0</reference-dist>
            <max-dist>2.0</max-dist>
        </gear>

        <gear>
            <name>gear-lock</name>
            <path>Sounds/gear-lck.wav</path>
            <condition>
                <equals>
                    <property>gear/gear[0]/position-norm</property>
                    <value>0</value>
                </equals>
            </condition>
            <reference-dist>2.0</reference-dist>
            <max-dist>5.0</max-dist>
            <position>
                <x>-3</x>
                <y>0</y>
                <z>0.5</z>
            </position>
            <reference-dist>1.0</reference-dist>
            <max-dist>2.0</max-dist>
        </gear>

        <squeal>
            <name>squeal1</name>
            <mode>once</mode>
            <reference-dist>20.0</reference-dist>
            <max-dist>40.0</max-dist>
            <path>Sounds/squeal2.wav</path>
            <condition>
                <and>
                    <property>gear/gear[6]/wow</property>
                    <property>gear/gear[6]/ground-is-solid</property>
                </and>
            </condition>
            <volume>
                <property>gear/gear[6]/wow</property>
            </volume>
            <volume>
                <property>sim/sound/cabin</property>
            </volume>
            <volume>
                <property>velocities/airspeed-kt</property>
                <factor>0.02</factor>
            </volume>
            <position>
                <x>0</x>
                <y>0</y>
                <z>-2</z>
            </position>
        </squeal>

        <squeal>
            <name>squeal2</name>
            <mode>once</mode>
            <reference-dist>20.0</reference-dist>
            <max-dist>40.0</max-dist>
            <path>Sounds/squeal2.wav</path>
            <condition>
                <and>
                    <property>gear/gear[7]/wow</property>
                    <property>gear/gear[7]/ground-is-solid</property>
                </and>
            </condition>
            <volume>
                <property>gear/gear[7]/wow</property>
            </volume>
            <volume>
                <property>sim/sound/cabin</property>
            </volume>
            <volume>
                <property>velocities/airspeed-kt</property>
                <factor>0.02</factor>
            </volume>
            <position>
                <x>0</x>
                <y>0</y>
                <z>-2</z>
            </position>
        </squeal>

        <flap>
            <name>flaps</name>
            <mode>in-transit</mode>
            <path>Sounds/flaps.wav</path>
            <property>surface-positions/flap-pos-norm</property>
            <volume>
                <property>sim/sound/cabin</property>
            </volume>
            <position>
                <x>-3</x>
                <y>0</y>
                <z>0.5</z>
            </position>
            <reference-dist>2.0</reference-dist>
            <max-dist>5.0</max-dist>
        </flap>

        <wind>
            <name>wind</name>
            <condition>
                <property>sim/current-view/internal</property>
            </condition>
            <mode>looped</mode>
            <path>Sounds/wind.wav</path>
            <volume>
                <property>sim/sound/cabin</property>
            </volume>
            <position>
                <x>-5</x>
                <y>0</y>
                <z>0</z>
            </position>
            <volume>
                <property>position/altitude-ft</property>
                <factor>-0.000015</factor>
                <offset>1.0</offset>
                <min>0</min>
                <max>0.5</max>
            </volume>
            <volume>
                <property>velocities/airspeed-kt</property>
                <factor>0.001</factor>
                <min>0.0</min>
                <max>1</max>
            </volume>
            <pitch>
                <property>velocities/airspeed-kt</property>
                <factor>0.01</factor>
                <offset>1.0</offset>
            </pitch>
            <position>
                <x>-3</x>
                <y>0</y>
                <z>0.5</z>
            </position>
            <reference-dist>1.0</reference-dist>
            <max-dist>20.0</max-dist>
        </wind>

        <!-- Radio -->
        <radio>
            <name>sound-radio-inside-comm0</name>
            <mode>looped</mode>
            <path>Aircraft/dhc6/Sounds/dhc6-radio-5s.wav</path>
            <condition>
                <and>
                    <equals>
                        <property>instrumentation/comm/serviceable</property>
                        <value>true</value>
                    </equals>
                    <greater-than>
                        <property>systems/electrical/outputs/comm[0]</property>
                        <value>1</value>
                    </greater-than>
                    <property>instrumentation/comm/ptt</property>
                    <value>0</value>
                </and>
            </condition>
            <volume>
                <factor>1</factor>
            </volume>
            <reference-dist>100</reference-dist>
            <max-dist>1000</max-dist>
        </radio>

        <!--  BRAKES   -->
        <brakes>
            <name>brake-lever-off</name>
            <mode>once</mode>
            <path>Aircraft/dhc6/Sounds/dhc6-brake-lever.wav</path>
            <condition>
                <not>
                    <property>controls/gear/parkingbrake-lever</property>
                </not>
            </condition>
            <volume>
                <property>sim/signals/fdm-initialized</property>
            </volume>
            <pitch>
                <offset>0.8</offset>
            </pitch>
            <position>
                <x>3</x>
                <y>0</y>
                <z>1</z>
            </position>
            <reference-dist>1</reference-dist>
            <max-dist>20</max-dist>
        </brakes>

        <brakes>
            <name>brake-lever-on</name>
            <mode>once</mode>
            <path>Aircraft/dhc6/Sounds/dhc6-brake-lever.wav</path>
            <condition>
                <and>
                    <property>controls/gear/parkingbrake-lever</property>
                    <value>0</value>
                </and>
            </condition>
            <volume>
                <property>sim/signals/fdm-initialized</property>
            </volume>
            <pitch>
                <offset>0.7</offset>
            </pitch>
            <position>
                <x>3</x>
                <y>0</y>
                <z>1</z>
            </position>
            <reference-dist>1</reference-dist>
            <max-dist>20</max-dist>
        </brakes>

        <!-- RUDDER  -->
       <rudder>
            <name>rudder</name>
            <mode>looped</mode>
            <path>Aircraft/dhc6/Sounds/dhc6-rudder.wav</path>
            <property>controls/flight/rudder</property>
            <condition>             
                <less-than>
                    <property>controls/engines/internal-engine-starter</property>
                    <value>0.001</value>
                </less-than>
                <not>
                    <property>sim/current-view/internal</property>
                </not>            
                <or>
                    <property>gear/gear[0]/wow</property>
                    <property>gear/gear[1]/wow</property>
                    <property>gear/gear[2]/wow</property>
