                        <value>0</value>
                    </greater-than>
                    <or>
                        <not>
                            <property>/sim/current-view/internal</property>
                        </not>
                        <greater-than>
                            <property>/controls/doors/LF-door/position-norm</property>
                            <value>0.05</value>
                        </greater-than>
                        <greater-than>
                            <property>/controls/doors/RF-door/position-norm</property>
                            <value>0.05</value>
