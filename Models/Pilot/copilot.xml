<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>copilot.ac</path>

    <animation>
        <type>select</type>
        <object-name>headset</object-name>
        <object-name>glasses</object-name>
        <object-name>Head</object-name>
        <object-name>Torso</object-name>
        <object-name>Rarm</object-name>
        <object-name>Larm</object-name>
        <object-name>Rforearm</object-name>
        <object-name>Lforearm</object-name>
        <object-name>Pelvis</object-name>
        <object-name>Rthigh</object-name>
        <object-name>Lthigh</object-name>
        <object-name>Rcalf</object-name>
        <object-name>Lcalf</object-name>
        <object-name>Rfoot</object-name>
        <object-name>Lfoot</object-name>
        <condition>
            <not>
                <property>sim/walker/outside</property>
            </not>
        </condition>
    </animation>

    <animation>
        <object-name>headset</object-name>
        <object-name>glasses</object-name>
        <object-name>Head</object-name>
        <object-name>Torso</object-name>
        <object-name>Rarm</object-name>
        <object-name>Larm</object-name>
        <object-name>Rforearm</object-name>
        <object-name>Lforearm</object-name>
        <object-name>Pelvis</object-name>
        <object-name>Rthigh</object-name>
        <object-name>Lthigh</object-name>
        <object-name>Rcalf</object-name>
        <object-name>Lcalf</object-name>
        <object-name>Rfoot</object-name>
        <object-name>Lfoot</object-name>
        <nopreview/>
    </animation>

    <animation>
        <type>select</type>
        <object-name>headset</object-name>
        <object-name>glasses</object-name>
        <object-name>Head</object-name>
        <condition>
            <not-equals>
                <property>sim/current-view/view-number</property>
                <value>13</value>
            </not-equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>headset</object-name>
        <object-name>glasses</object-name>
        <object-name>Head</object-name>
        <object-name>Torso</object-name>
        <object-name>Rarm</object-name>
        <object-name>Larm</object-name>
        <object-name>Rforearm</object-name>
        <object-name>Lforearm</object-name>
        <object-name>Pelvis</object-name>
        <object-name>Rthigh</object-name>
        <object-name>Lthigh</object-name>
        <object-name>Rcalf</object-name>
        <object-name>Lcalf</object-name>
        <object-name>Rfoot</object-name>
        <object-name>Lfoot</object-name>
        <condition>
            <or>
                <and>
                    <property>sim/pilots/pilots-enabled</property>
                    <property>sim/pilots/show-copilot</property>
                    <not>
                        <property>sim/pilots/hide-copilot-cockpit-view</property>
                    </not>
                    <equals>
                        <property>sim/current-view/view-number</property>
                        <value>13</value>
                    </equals>
                </and>
                <and>
                    <property>sim/pilots/pilots-enabled</property>
                    <property>sim/pilots/show-copilot</property>
                    <not-equals>
                        <property>sim/current-view/view-number</property>
                        <value>13</value>
                    </not-equals>
                </and>
            </or>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>glasses</object-name>
        <condition>
            <less-than>
                <property>/sim/time/sun-angle-rad</property>
                <value>1.57</value>
            </less-than>
        </condition>
    </animation>


    <animation>
        <type>select</type>
        <object-name>headset</object-name>
        <condition>
            <or>
                <property>engines/engine/running</property>
                <property>engines/engine[1]/running</property>
            </or>
        </condition>
    </animation>

    <!-- Arms: elevator -->
    <animation>
        <type>rotate</type>
        <object-name>Lforearm</object-name>
        <object-name>Rforearm</object-name>
        <property>controls/flight/elevator</property>
        <factor>10</factor>
        <center>
            <x-m>-0.52</x-m>
            <y-m>0.06</y-m>
            <z-m>-0.36</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>glasses</object-name>
        <object-name>headset</object-name>
        <object-name>Pelvis</object-name>
        <object-name>Torso</object-name>
        <object-name>Head</object-name>
        <object-name>Larm</object-name>
        <object-name>Rarm</object-name>
        <property>controls/flight/elevator</property>
        <condition>
            <greater-than-equals>
                <property>controls/flight/elevator</property>
                <value>0</value>
            </greater-than-equals>
        </condition>
        <factor>10</factor>
        <center>
            <x-m>0.05</x-m>
            <y-m>0.01</y-m>
            <z-m>0.02</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Larm</object-name>
        <object-name>Rarm</object-name>
        <property>controls/flight/elevator</property>
        <condition>
            <less-than-equals>
                <property>controls/flight/elevator</property>
                <value>0</value>
            </less-than-equals>
        </condition>
        <factor>28</factor>
        <center>
            <x-m>0.10</x-m>
            <y-m>0.01</y-m>
            <z-m>0.59</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Larm</object-name>
        <object-name>Rarm</object-name>
        <property>controls/flight/elevator</property>
        <condition>
            <greater-than-equals>
                <property>controls/flight/elevator</property>
                <value>0</value>
            </greater-than-equals>
        </condition>
        <factor>22</factor>
        <center>
            <x-m>0.10</x-m>
            <y-m>0.01</y-m>
            <z-m>0.59</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- Arms: aileron -->
    <animation>
        <type>rotate</type>
        <object-name>Lforearm</object-name>
        <property>controls/flight/aileron</property>
        <factor>-40</factor>
        <center>
            <x-m>-0.15</x-m>
            <y-m>-0.18</y-m>
            <z-m>0.41</z-m>
        </center>
        <axis>
            <x1-m>-0.419</x1-m>
            <y1-m> 0.070</y1-m>
            <z1-m> 0.300</z1-m>
            <x2-m>-0.147</x2-m>
            <y2-m>-0.183</y2-m>
            <z2-m> 0.413</z2-m>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Rforearm</object-name>
        <property>controls/flight/aileron</property>
        <factor>-40</factor>
        <center>
            <x-m>-0.15</x-m>
            <y-m>0.23</y-m>
            <z-m>0.41</z-m>
        </center>
        <axis>
            <x1-m>-0.419</x1-m>
            <y1-m> 0.070</y1-m>
            <z1-m> 0.300</z1-m>
            <x2-m>-0.147</x2-m>
            <y2-m> 0.231</y2-m>
            <z2-m> 0.413</z2-m>
        </axis>
    </animation>

    <!-- Head: elevator -->
    <animation>
        <type>rotate</type>
        <object-name>Head</object-name>
        <object-name>headset</object-name>
        <object-name>glasses</object-name>
        <property>controls/flight/elevator</property>
        <factor>8</factor>
        <center>
            <x-m>0.076</x-m>
            <y-m>0</y-m>
            <z-m>0.676</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- Head: aileron -->
    <animation>
        <type>rotate</type>
        <object-name>Head</object-name>
        <object-name>headset</object-name>
        <object-name>glasses</object-name>
        <property>controls/flight/aileron</property>
        <factor>30</factor>
        <center>
            <x-m>0.076</x-m>
            <y-m>0</y-m>
            <z-m>0.676</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>0.0</y>
            <z>-1.0</z>
        </axis>
    </animation>

    <!-- Feet -->
    <animation>
        <type>translate</type>
        <object-name>Lfoot</object-name>
        <property>controls/flight/rudder</property>
        <factor>0.025</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Lfoot</object-name>
        <property>controls/flight/rudder</property>
        <condition>
            <less-than-equals>
                <property>controls/flight/rudder</property>
                <value>0</value>
            </less-than-equals>
        </condition>
        <factor>10</factor>
        <center>
            <x-m>-0.98</x-m>
            <y-m>0.1</y-m>
            <z-m>-0.2</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Lcalf</object-name>
        <property>controls/flight/rudder</property>
        <factor>3</factor>
        <center>
            <x-m>-0.38</x-m>
            <y-m>0.1</y-m>
            <z-m>0.06</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>Rfoot</object-name>
        <property>controls/flight/rudder</property>
        <factor>0.025</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Rfoot</object-name>
        <property>controls/flight/rudder</property>
        <condition>
            <greater-than-equals>
                <property>controls/flight/rudder</property>
                <value>0</value>
            </greater-than-equals>
        </condition>
        <factor>10</factor>
        <center>
            <x-m>-0.98</x-m>
            <y-m>-0.12</y-m>
            <z-m>-0.2</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Rcalf</object-name>
        <property>controls/flight/rudder</property>
        <factor>3</factor>
        <center>
            <x-m>-0.38</x-m>
            <y-m>-0.12</y-m>
            <z-m>0.06</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>material</type>
        <object-name>Head</object-name>
        <object-name>Torso</object-name>
        <object-name>Rarm</object-name>
        <object-name>Larm</object-name>
        <object-name>Rforearm</object-name>
        <object-name>Lforearm</object-name>
        <object-name>Pelvis</object-name>
        <object-name>Rthigh</object-name>
        <object-name>Lthigh</object-name>
        <object-name>Rcalf</object-name>
        <object-name>Lcalf</object-name>
        <object-name>Rfoot</object-name>
        <object-name>Lfoot</object-name>
        <object-name>glasses</object-name>
        <object-name>headset</object-name>
        <emission>
            <red>0.381924</red>
            <green>0.30935844</green>
            <blue>0.161171928</blue>
            <factor-prop>sim/multiplay/generic/float[10]</factor-prop>
        </emission>
    </animation>

</PropertyList>
